{
  "name": "crawler",
  "version": "1.0.0",
  "description": "Crawl for jobs",
  "main": "index.js",
  "scripts": {
    "build-css": "stylus source/stylesheets/index.styl -o static/css",
    "watch-css": "stylus source/stylesheets/index.styl -o static/css -w",
    "clean": "rm -rf static/css && mkdir -p static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon server -e js,jade",
    "start": "node server"
  },
  "author": "Einar Ã“lafsson",
  "license": "ISC",
  "dependencies": {
    "JSONStream": "^1.1.4",
    "cheerio": "^0.20.0",
    "event-stream": "^3.3.4",
    "express": "^4.14.0",
    "jade": "^1.11.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.10.0",
    "request": "^2.74.0",
    "stream-json": "^0.4.2",
    "stylus": "^0.54.5"
  }
}
